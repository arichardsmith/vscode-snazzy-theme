{
  "name": "Snazzy vs theme",
  "type": "dark",
  "colors": {
    "sideBarTitle.foreground": "#bbbbbb",

    "foreground": "#eff0eb",
    "editorLink.activeForeground": "#ff5c57",
    "editor.background": "#282a36",
    "editorCursor.foreground": "#97979b",
    "editor.findMatchHighlightBackground": "#54555e",
    "editor.findRangeHighlightBackground": "#54555e",
    "editor.foreground": "#eff0eb",
    "editor.hoverHighlightBackground": "#54555e",
    "editorIndentGuide.background": "#616263",
    "editor.lineHighlightBackground": "#2C2F3D",
    "editorLineNumber.foreground": "#616263",
    "editor.rangeHighlightBackground": "#54555e",
    "editor.selectionHighlightBackground": "#54555e",
    "editorWhitespace.foreground": "#616263",
    "editor.wordHighlightBackground": "#54555e",
    "editor.wordHighlightStrongBackground": "#54555e",
    "peekView.border": "#5af78e",
    "peekViewEditor.background": "#282a36",
    "list.activeSelectionBackground": "#54555e",
    "list.hoverBackground": "#54555e",
    "statusBar.background": "#2E313D",
    "statusBar.foreground": "#6C6E75",
    "statusBar.debuggingBackground": "#6C6E75",
    "statusBar.noFolderBackground": "#6C6E75",
    "activityBar.background": "#282a36",
    "sideBar.background": "#282a36",
    "sideBar.border": "#2C2F3D",
    "tab.inactiveBackground": "#2C2F3D",
    "tab.activeBackground": "#282a36",
    "editorGroup.dropBackground": "#232631",
    "editorGroupHeader.tabsBackground": "#2C2F3D",
    "titleBar.activeBackground": "#282a36",
    "widget.shadow": "#282a36",

    "terminal.background": "#282a36",
    "terminal.foreground": "#eff0eb",
    "terminal.ansiBlack": "#282a36",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiGreen": "#5af78e",
    "terminal.ansiYellow": "#f3f99d",
    "terminal.ansiBlue": "#57c7ff",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiWhite": "#f1f1f0",
    "terminal.ansiBrightBlack": "#686868",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightGreen": "#5af78e",
    "terminal.ansiBrightYellow": "#f3f99d",
    "terminal.ansiBrightBlue": "#57c7ff",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightWhite": "#eff0eb",

    "notification.background": "#282a36",
    "notification.foreground": "#eff0eb",
    "notification.buttonBackground": "#57c7ff",
    "notification.buttonHoverBackground": "#9aedfe",
    "notification.buttonForeground": "#f1f1f0",
    "notification.infoBackground": "#9aedfe",
    "notification.infoForeground": "#eff0eb",
    "notification.warningBackground": "#f3f99d",
    "notification.warningForeground": "#eff0eb",
    "notification.errorBackground": "#ff5c57",
    "notification.errorForeground": "#eff0eb",

    "editorError.foreground": "#ff5c57",
    "editorWarning.foreground": "#f3f99d",

    "badge.background": "#ff5c57",
    "badge.foreground": "#f1f1f0",

    "activityBarBadge.background": "#57c7ff",
    "activityBarBadge.foreground": "#f1f1f0",

    "sideBarSectionHeader.background": "#282a36",
    "sideBarSectionHeader.foreground": "#f1f1f0",

    "dropdown.border": "#2C2F3D"
  },
  "tokenColors": [{
      "settings": {
        "background": "#282a36",
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Function Argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "Storage Modifier [e.g. class, extends, static] (JS)",
      "scope": "storage.modifier, storage.type.class.js",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "this keyword",
      "scope": "variable.language.this, variable.language.this.js",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Emphasis",
      "scope": "emphasis, markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "function",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "React component",
      "scope": [
        "support.class" // Componente react
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    }
  ]
}
