  "tokenColors": [
      {
          "name": "HTML Attributes",
          "scope": [
              "text.html.basic entity.other.attribute-name",
              "text.html.basic entity.other.attribute-name.html"
          ],
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
          "settings": {
              "background": "#282a36",
              "foreground": "#eff0eb"
          }
      },
      {
          "name": "Variables",
          "scope": [
              "string constant.other.placeholder",
              "variable"
          ],
          "settings": {
              "foreground": "#EEFFFF"
          }
      },
      {
          "name": "Colors",
          "scope": [
              "constant.other.color"
          ],
          "settings": {
              "foreground": "#eff0eb"
          }
      },
      {
          "name": "Invalid",
          "scope": [
              "invalid",
              "invalid.illegal"
          ],
          "settings": {
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "Keyword",
          "scope": [
              "keyword"
          ],
          "settings": {
              "foreground": "#ff6ac1"
          }
      },
      {
          "name": "Storage",
          "scope": [
              "storage.modifier",
              "storage.type"
          ],
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
          "name": "Operator, Misc",
          "scope": [
              "constant.other.color",
              "keyword.control",
              "keyword.other.substitution",
              "keyword.other.template",
              "punctuation",
              "punctuation.definition.tag",
              "punctuation.definition.tag.begin.html",
              "punctuation.definition.tag.end.html",
              "punctuation.definition.tag.html",
              "punctuation.section.embedded",
              "punctuation.separator.inheritance.php"
          ],
          "settings": {
              "foreground": "#89DDFF"
          }
      },
      {
          "name": "Html text",
          "scope": [
              "meta.tag"
          ],
          "settings": {
              "foreground": "#eff0eb"
          }
      },
      {
          "name": "Tag",
          "scope": [
              "entity.name.tag",
              "markup.deleted.git_gutter",
              "meta.tag.sgml"
          ],
          "settings": {
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "Function, Special Method",
          "scope": [
              "entity.name.function",
              "keyword.other.special-method",
              "meta.function-call",
              "support.function",
              "variable.function"
          ],
          "settings": {
              "foreground": "#57c7ff"
          }
      },
      {
          "name": "Block Level Variables",
          "scope": [
              "meta.block variable.other"
          ],
          "settings": {
              "foreground": "#eff0eb"
          }
      },
      {
          "name": "Other Variable, String Link",
          "scope": [
              "string.other.link",
              "support.other.variable"
          ],
          "settings": {
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "Number, Constant, Tag Attribute, Embedded",
          "scope": [
              "constant.character",
              "constant.escape",
              "constant.language",
              "constant.numeric",
              "keyword.other",
              "keyword.other.unit",
              "support.constant"
          ],
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
        "name": "CSS Value",
        "scope": [
            "keyword.other.unit.css",
            "constant.numeric.css",
            "support.constant.property-value.css"
        ],
        "settings": {
            "foreground": "#eff0eb"
        }
      },
      {
          "name": "Function Argument",
          "scope": [
              "variable.parameter"
          ],
          "settings": {
              "foreground": "#eff0eb"
          }
      },
      {
          "name": "String, Symbols, Inherited Class, Markup Heading",
          "scope": [
              "constant.other.key",
              "constant.other.symbol",
              "entity.other.inherited-class",
              "markup.heading",
              "markup.inserted.git_gutter",
              "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
              "string"
          ],
          "settings": {
              "fontStyle": "normal",
              "foreground": "#5af78e"
          }
      },
      {
          "name": "Class, Support",
          "scope": [
              "entity.name",
              "markup.changed.git_gutter",
              "meta.use.php",
              "support.class",
              "support.orther.namespace.use.php",
              "support.other.namespace.php",
              "support.type",
              "support.type.sys-types"
          ],
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
          "name": "Entity Types",
          "scope": [
              "support.type"
          ],
          "settings": {
              "foreground": "#B2CCD6"
          }
      },
      {
          "name": "CSS Class and Support",
          "scope": [
              "source.css support.type.property-name",
              "source.less support.type.property-name",
              "source.postcss support.type.property-name",
              "source.sass support.type.property-name",
              "source.scss support.type.property-name",
              "source.stylus support.type.property-name"
          ],
          "settings": {
              "foreground": "#9aedfe"
          }
      },
      {
          "name": "Sub-methods",
          "scope": [
              "entity.name.module.js",
              "variable.import.parameter.js",
              "variable.other.class.js"
          ],
          "settings": {
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "Language methods",
          "scope": [
              "variable.language"
          ],
          "settings": {
              "fontStyle": "italic",
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "entity.name.method.js",
          "scope": [
              "entity.name.method.js"
          ],
          "settings": {
              "fontStyle": "italic",
              "foreground": "#82AAFF"
          }
      },
      {
          "name": "meta.method.js",
          "scope": [
              "meta.class-method.js entity.name.function.js",
              "variable.function.constructor"
          ],
          "settings": {
              "foreground": "#82AAFF"
          }
      },
      {
          "name": "Attributes",
          "scope": [
              "entity.other.attribute-name"
          ],
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
          "name": "Comment",
          "scope": [
              "comment",
              "punctuation.definition.comment"
          ],
          "settings": {
              "fontStyle": "italic",
              "foreground": "#546E7A"
          }
      },
      {
          "name": "CSS Classes",
          "scope": [
              "entity.other.attribute-name.class"
          ],
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
          "name": "CSS ID's",
          "scope": [
              "source.sass keyword.control"
          ],
          "settings": {
              "foreground": "#82AAFF"
          }
      },
      {
          "name": "Inserted",
          "scope": [
              "markup.inserted"
          ],
          "settings": {
              "foreground": "#C3E88D"
          }
      },
      {
          "name": "Deleted",
          "scope": [
              "markup.deleted"
          ],
          "settings": {
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "Changed",
          "scope": [
              "markup.changed"
          ],
          "settings": {
              "foreground": "#C792EA"
          }
      },
      {
          "name": "Regular Expressions",
          "scope": [
              "string.regexp"
          ],
          "settings": {
              "foreground": "#89DDFF"
          }
      },
      {
          "name": "Escape Characters",
          "scope": [
              "constant.character.escape"
          ],
          "settings": {
              "foreground": "#89DDFF"
          }
      },
      {
          "name": "URL",
          "scope": [
              "*link*",
              "*uri*",
              "*url*"
          ],
          "settings": {
              "fontStyle": "underline"
          }
      },
      {
          "name": "Decorators",
          "scope": [
              "tag.decorator.js entity.name.tag.js",
              "tag.decorator.js punctuation.definition.tag.js"
          ],
          "settings": {
              "fontStyle": "italic",
              "foreground": "#82AAFF"
          }
      },
      {
          "name": "ES7 Bind Operator",
          "scope": [
              "source.js constant.other.object.key.js string.unquoted.label.js"
          ],
          "settings": {
              "fontStyle": "italic",
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "JSON Key",
          "scope": [
              "source.json meta.structure.dictionary.json support.type.property-name.json"
          ],
          "settings": {
              "foreground": "#FFCB6B"
          }
      },
      {
          "scope": "token.info-token",
          "settings": {
              "foreground": "#6796e6"
          }
      },
      {
          "scope": "token.warn-token",
          "settings": {
              "foreground": "#f3f99d"
          }
      },
      {
          "scope": "token.error-token",
          "settings": {
              "foreground": "#ff5c57"
          }
      },
      {
          "scope": "token.debug-token",
          "settings": {
              "foreground": "#b267e6"
          }
      },
      {
          "name": "Storage Modifier [e.g. class, extends, static] (JS)",
          "scope": "storage.modifier, storage.type.class.js",
          "settings": {
              "fontStyle": "italic",
              "foreground": "#ff6ac1"
          }
      },
      {
          "name": "this keyword",
          "scope": "variable.language.this, variable.language.this.js",
          "settings": {
              "fontStyle": "italic",
              "foreground": "#ff5c57"
          }
      },
      {
          "name": "Emphasis",
          "scope": "emphasis, markup.italic",
          "settings": {
              "fontStyle": "italic"
          }
      },
      {
          "name": "Keyword Control",
          "scope": [
              "keyword.control"
          ],
          "settings": {
              "foreground": "#ff6ac1"
          }
      },
      {
          "name": "function",
          "scope": [
              "entity.name.function"
          ],
          "settings": {
              "foreground": "#57c7ff"
          }
      },
      {
          "name": "Variables",
          "scope": [
              "meta.definition.variable",
              "support.variable",
              "variable"
          ],
          "settings": {
              "foreground": "#eff0eb"
          }
      },
      {
          "name": "React component",
          "scope": [
              "support.class"
          ],
          "settings": {
              "foreground": "#FFCB6B"
          }
      }
  ]
}
